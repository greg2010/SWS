{% extends "baseNoAuth.twig" %}

{% block title %}Registration{% endblock %}

{% block content %}
<form>
   <h1>REGISTRATION</h1>
     <div class="inset">
        <p>
            <label for="username">KEY ID</label>
            <input type="text" name="usermane" id="username">
        </p>
        <p>
            <label for="vCode">VERIFICATION CODE</label>
            <input type="text" name="vCode" id="vCode">
        </p>
        <p class="p-container">
               <a href="https://support.eveonline.com/api/key/createpredefined/8">Get Key ID & vCode</a>                         
               <input type="submit" name="go" id="go" value="Get Characters">
        </p>
        <hr />
        <p>
            <label for="email">E-MAIL (OPTIONAL)</label>
            <input type="text" name="email" id="email">
        </p>
        <hr />
        <p>
            <label for="password">PASSWORD</label>
            <input type="password" name="password" id="password">
        </p>
        <p>
            <label for="password">REPEAT PASSWORD</label>
            <input type="password" name="password" id="password">
        </p>
        <hr />
        <p class="p-container">
            <input type="submit" name="go" id="go" value="SUBMIT">
        </p>
      </div>
</form>
{% endblock %}